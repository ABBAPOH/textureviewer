<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
	<mime-type type="image/x-vtf">
		<magic priority="100">
			<match type="string" offset="0" value="VTF\0"/>
		</magic>
		<glob pattern="*.vtf"/>
		<comment>Valve Texture Format</comment>
	</mime-type>
</mime-info>
